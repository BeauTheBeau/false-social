{
  "name": "false-social",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www | bunyan -o short",
    "dev": "nodemon ./bin/www --ignore 'src/data/*' | bunyan -o short",
    "format": "prettier --write src/**/*.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "cookie-parser": "~1.4.6",
    "debug": "~4.3.4",
    "dotenv": "^16.3.1",
    "express": "~4.18.2",
    "express-handlebars": "^7.1.2",
    "handlebars": "^4.7.8",
    "hbs": "~4.2.0",
    "http-errors": "~2.0.0",
    "morgan": "~1.10.0",
    "node-sass-middleware": "1.1.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "nodemon": "^3.0.2",
    "bunyan": "^1.8.15",
    "prettier": "^3.1.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
